@model IEnumerable<AnnouncementServiceModel>

@{
	ViewBag.Title = "My announced problems";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (!Model.Any())
{
	<h2 class="text-center">You have not announced any problems yet!</h2>
}

<div class="row">
	@foreach (var announcement in Model)
	{
		<partial name="_AnnouncementPartial" model="@announcement" />
	}
</div>
